{{#if model.length}}
<ul class="bookmark-list">
  {{#each model as |bookmark|}}
    <li class="bookmeister-bookmark">
      <a class="title" href={{bookmark.url}}>
        {{bookmark.displayName}}
      </a>

      <div class="metadata">
        {{date-stamp bookmark.createdAt}}

        {{#link-to 'bookmarks.edit' bookmark}}edit{{/link-to}}
        {{#link-to 'bookmarks.delete' bookmark}}delete{{/link-to}}
      </div>
    </li>
  {{/each}}
</ul>
{{/if}}

{{page-numbers content=model}}
