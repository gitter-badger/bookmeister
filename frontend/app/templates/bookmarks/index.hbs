{{#if controller.length}}
<ul class="bookmark-list">
  {{#each controller}}
    <li class="bookmeister-bookmark">
      <a class="title" {{bind-attr href=url}}>
        {{displayName}}
      </a>

      <div class="metadata">
        {{date-stamp createdAt}}

        {{#link-to 'bookmarks.edit' this}}edit{{/link-to}}
        {{#link-to 'bookmarks.delete' this}}delete{{/link-to}}
      </div>
    </li>
  {{/each}}
</ul>
{{/if}}

{{controller.length}} bookmarks found.
